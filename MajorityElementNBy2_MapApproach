int main()
{
    unordered_map<int,int> umap;
    vector<int> nums = {1,1,2,2,2,2,1,2};
    int nm = nums.size()/2;
    for(int i=0;i<nums.size();i++)
    {
        umap[nums[i]]++; 
        if(umap[nums[i]] > nm) { break; }
    }
    
    for(auto pair: umap)
    {
        if(pair.second>nm)
        {
            cout<<pair.first;
        }
    }
    
	return 0;
}
