#include <iostream>
#include <bits/stdc++.h>
#include <vector>
using namespace std;

int main()
{
    vector<int> nums = {10,22,12,3,0,6};
    int n = nums.size();
    cout << "Leaders are : " << nums[n-1] << " ";
    int idx = -1;
    int maxSoFar = nums[n-1];
    for(int i=n-2;i>=0;i--)
    {
        if(nums[i] > maxSoFar)
        {
            maxSoFar = nums[i];
            cout << nums[i] << " " ; 
        }
    }

    return 0;
}
