#include <iostream>
#include <vector>
#include <unordered_map>
#include <algorithm>
using namespace std;

void bruteForce(vector<int> nums, int target)
{
    for(int i=0;i<nums.size()-1;i++)
    {
        int sum = 0;
        for(int j=1;i<nums.size();i++)
        {
            sum = nums[i] + nums[j];
            if(sum == target)
            {
                cout << nums[i] << " " << nums[j] << endl;
            }
        }    
    }
}

void better(vector<int> nums, int target)
{
    unordered_map<int,int> umap;
    for(int i=0;i<nums.size()-1;i++)
    {
        int rem = target - nums[i];
        if(umap.find(rem) != umap.end())
        {
            cout << nums[i] << " " << rem << endl;    
        }
        
        umap[nums[i]] = i;
    }
}

void optimal(vector<int> nums, int target)
{
    sort(nums.begin(), nums.end());
    int i=0;
    int j =nums.size()-1;
    while(i<j)
    {
        int sum = nums[i] + nums[j];
        if(sum < target)
            i++;
        if(sum > target)
            j--;
        if(sum == target)
        {
            cout << nums[i] << " " << nums[j] << endl;
            break;    
        }
    }
}

int main()
{
    int target = 14;
    vector<int> nums = {2,6,5,8,11};
    bruteForce(nums,target);
    better(nums,target);
    optimal(nums,target);
    return 0;
}
