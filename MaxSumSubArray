#include <iostream>
#include <vector>
#include <unordered_map>
#include <algorithm>
#include <climits>
using namespace std;

int maximumSumSubArray(vector<int> nums,int n) {
    int maxSum = INT_MIN;
    for(int i=0;i<n;i++)
    {
        int sum = 0;
        for(int j=i;j<n;j++)
        {
            sum+=nums[j];
            maxSum = max(maxSum,sum); 
        }
    }
    cout << maxSum << endl;
    return maxSum;
}
int maximumSumSubArrayKadane(vector<int> nums,int n) {
    int maxSum = INT_MIN;
    int sum = 0;
    int start = -1;
    int end = -1;
    for(int i=0;i<n;i++)
    {
        if(sum == 0)
        {
            start = i;
        }
        
        sum+=nums[i];
        if(maxSum < sum)
        {
            maxSum = sum;
            end = i;
        }
        
        if(sum<0)
        {
            sum = 0;
        }
    }
    
    cout << start << " " << end << endl;
    cout << maxSum << endl;
    return maxSum;
}
int main()
{
    vector<int> nums =  {2, 3, -8, 7, -1, 2, 3};

    int n = nums.size();
    maximumSumSubArray(nums,n);
    maximumSumSubArrayKadane(nums,n);
    return 0;
}
