#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

vector<vector<int>> mergeOverLappingIntervalsBetterTUF(vector<vector<int>> pairs)
{
    vector<vector<int>> result;
    for(int i=0;i<pairs.size();i++)
    {
        if(result.empty() || pairs[i][0] > result.back()[1])
        {
            result.push_back(pairs[i]);    
        }
        else{
            result.back()[1] = max(pairs[i][1],result.back()[1]);
        }
    }
    return result;
}

vector<vector<int>> mergeOverLappingIntervals(vector<vector<int>> pairs)
{
    vector<vector<int>> result;
    int start = pairs[0][0];
    int end   = pairs[0][1];
    for(int i=1;i<pairs.size();i++)
    {
        if(pairs[i][0] <= end)
        {
            if(end<pairs[i][1])
            {
                end = pairs[i][1];
            }
        }
        else
        {
            result.push_back({start,end});   
            start = pairs[i][0];
            end = pairs[i][1];
        }
    }
    
    result.push_back({start,end}); 
    return result;
}

vector<vector<int>> mergeOverLappingIntervalsBFTUF(vector<vector<int>> pairs)
{
    vector<vector<int>> result;
    for(int i=0;i<pairs.size();i++)
    {
        int start = pairs[i][0];    
        int end = pairs[i][1];
        
        if(i>0 && start<=result.back()[1]) {continue;}
        
        for(int j=i+1;j<pairs.size();j++)
        {
            if(pairs[j][0] <= end)
            {
                if(pairs[j][1] > end)
                {
                    end = pairs[j][1];
                }
            }
            else
            {
                break;
            }
        }
        
        result.push_back({start,end});
    }
    
    return result;
}

int main()
{
    vector<vector<int>> pairs = {{2,3},{2,2},{3,3},{1,3},{5,7},{2,2},{4,5}};
    //{1,3},{2,2},{2,2},{2,3},{3,3},{4,5},{5,7}
    sort(pairs.begin(),pairs.end());
    vector<vector<int>> result = mergeOverLappingIntervals(pairs);
    vector<vector<int>> result1 = mergeOverLappingIntervalsBFTUF(pairs);
    vector<vector<int>> result2 = mergeOverLappingIntervalsBetterTUF(pairs);
    for (auto interval : result)
        cout << interval[0] << " " << interval[1] << endl;
        
    for (auto interval : result1)
        cout << interval[0] << " " << interval[1] << endl;

    for (auto interval : result)
        cout << interval[0] << " " << interval[1] << endl;        
    return 0;
}
