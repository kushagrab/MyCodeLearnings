#include <iostream>
#include <vector>
using namespace std;
int searchInsert(vector<int>& nums, int target) 
{
    int low =0;
    int high = nums.size()-1;
    int mid = -1;
    int ans;
    while(low<=high)
    {
        mid = (low+high)/2;
        if(nums[mid] > target) //upper bound for lower bound just add >=
        {
            ans = mid;
            high = mid-1;
        }
        else
        {
            low = mid+1;
        }
    }

    return ans;
}

int main()
{
    vector<int> nums = {1,3,4,5,6};
    cout << searchInsert(nums,4);
    return 0;
}
