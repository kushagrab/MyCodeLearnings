#include <iostream>
#include <bits/stdc++.h>
#include <vector>
#include <unordered_set>
using namespace std;

int main()
{
	vector<vector<int>> matrix = {{1,1,1,1},{1,0,1,1},{1,1,0,1},{1,1,1,1}};
	int n = matrix.size();
	int m = matrix[0].size();
	vector<int> row(n,1);
	vector<int> col(m,1);
	
	for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
    
    cout << endl;
	
    for(int i=0;i<m;i++)
    {
        for(int j=0;j<n;j++)
        {
            if(matrix[i][j] == 0)
            {
                row[j] = 0;
                col[i] = 0;
            }
        }
    }
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            if(!row[j] || !col[i])
            {
                matrix[i][j] = 0;
            }
        }
    }
    
    for(int i=0;i<n;i++)
    {
        for(int j=0;j<m;j++)
        {
            cout << matrix[i][j] << " ";
        }
        cout << endl;
    }
    
	return 0;
}
