#include <iostream>
#include <bits/stdc++.h>
#include <vector>
#include <unordered_set>
#include <algorithm>
using namespace std;

int main()
{
    vector<int> nums = {1,1,2,2,2,2,1,2};
    int nm = nums.size()/2;
    int count = 0;
    int el;
    for(int i=0;i<nums.size();i++)
    {
        if(count==0)
        {
            el = nums[i];
            count++;
        }
        else if(el == nums[i])
        {
            count++;
        }
        else
        {
            count--;
        }
    }
    
    cout << el;
	return 0;
}
